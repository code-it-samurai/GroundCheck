<% if @sus_user %>
	<% controller.redirect_to root_path, notice: "Unauthorized Access" %>
<% else %>
	<div class="user-profile-edit-background">
		<% if user_signed_in? %>
		<%= render 'pages/header'%>
		<% else %>
			<%= render 'pages/guestnave'%>
		<% end %>

		<%= render 'form', user: @user %>

		<%= link_to 'Show', @user %> |
		<%= link_to 'Back', users_path %>
	</div>
<% end %>